   , sepChar = "%"
   , alignSep = "}{"
   , template =
      " %logo%  <fc=#a0a0a0><fn=3>%uname%</fn></fc><fc=#404040> ‚îÇ\
      \</fc><fc=#a0a0a0> %date%</fc><fc=#404040> ‚îÇ \
